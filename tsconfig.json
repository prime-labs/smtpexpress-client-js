{
  "include": ["src/index.ts", "src/helpers/*", "src/apis/*"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2015",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "skipLibCheck": true,
    // "module": "CommonJS",
    // "moduleResolution": "Node",
    "module": "AMD",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
}